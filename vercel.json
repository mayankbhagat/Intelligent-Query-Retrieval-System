{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "HACKRX_AUTH_TOKEN": "@hackrx_auth_token"
  }
}